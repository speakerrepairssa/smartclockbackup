{
  "name": "aiclock-dataconnect-migration",
  "version": "1.0.0",
  "description": "Firebase Data Connect migration tools for AiClock",
  "type": "module",
  "scripts": {
    "migrate": "node scripts/migrate-to-dataconnect.js",
    "export-only": "node scripts/export-firestore-data.js",
    "validate": "node scripts/validate-migration.js",
    "setup-dataconnect": "firebase dataconnect:services:create --service-id=aiclock-service --location=us-central1"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.6.0"
  },
  "devDependencies": {
    "firebase-tools": "^13.0.0"
  },
  "keywords": [
    "firebase",
    "data-connect",
    "postgresql", 
    "migration",
    "attendance-tracking"
  ]
}